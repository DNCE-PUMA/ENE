<sql> 
    <sql_ccpp> 
        CREATE TABLE IF NOT EXISTS T_CCPP( 		
        	ID INT  NULL,		
            UBIGEO CHAR(6) NOT NULL,		
            CCDD CHAR(2) NOT NULL,		
			DEPARTAMENTO VARCHAR(200) NULL,
			CCPP CHAR(2) NOT NULL,		
			PROVINCIA VARCHAR(200) NULL,
			CCDI CHAR(2) NOT NULL,		
			DISTRITO VARCHAR(200) NULL,
			CODCCPP CHAR(4) NOT NULL,	
			NOMCCPP VARCHAR(200) NULL,
			USUCRE VARCHAR(35) NULL,
			FECCRE VARCHAR(35) NULL,
			USUREG VARCHAR(35) NULL,
			FECREG VARCHAR(35) NULL,
			FECENV VARCHAR(35) NULL,
            PRIMARY KEY(CCDD,CCPP,CCDI, CODCCPP) 
        ) 
    </sql_ccpp>       
</sql> 
