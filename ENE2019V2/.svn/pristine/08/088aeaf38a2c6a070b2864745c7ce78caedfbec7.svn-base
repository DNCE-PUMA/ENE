					&& rgC4P446B_2.isTagSelected(2)
					&& rgC4P446B_3.isTagSelected(2)
					&& rgC4P446B_4.isTagSelected(2)) {
				mensaje = "Verificar: La Preg.446B Debe haber alguna opción “Si” seleccionada";
				view = rgC4P446B_1;
				error = true;
				return false;
			}		
			
		}

		return true;
	}

