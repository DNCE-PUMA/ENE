<sqldatabase> 
    <sql_tablas>
        CREATE TABLE T_MARCO (
        	ID INT NOT NULL,
		    RUC VARCHAR(11) NOT NULL,
		    RAZ_SOCIAL_LOCAL VARCHAR(200) NULL,
		    UBIGEO CHAR(6) NOT NULL,
		    CCDD CHAR(2) NULL,
		    NOMBREDD VARCHAR(100) NULL,
		    CCPP CHAR(2) NULL,
		    NOMBREPP VARCHAR(100) NULL,
		    CCDI CHAR(2) NULL,
		    NOMBREDI VARCHAR(100) NULL,
		    ODEI_CD CHAR(2) NULL,
		    ODEI VARCHAR(200) NULL,
		    NROESTAB INT NULL,
		    ZONA VARCHAR(5) NULL,
		    MZA VARCHAR(4) NULL,
		    FRENTE INT NULL,
		    M_TIPOVIA_CD INT NULL ,
			M_TIPOVIA VARCHAR(10) NULL, 
			M_NOMBREVIA VARCHAR(200) NULL, 
			M_PUERTA VARCHAR(5) NULL ,
	 		M_PISO VARCHAR(5) NULL ,
			M_INTERIOR VARCHAR(5) NULL,	
			M_MANZANA VARCHAR(3) NULL,
			M_LOTE VARCHAR(3) NULL,  
			M_REFERENCIA VARCHAR(200) NULL,
		    ANIO_CONSTITUCION INT NULL,
		    ANIO_APERTURA INT NULL,
		    ORGANIZACION_LEGAL_CD VARCHAR(2) NULL,
		    ORGANIZACION_LEGAL VARCHAR(200) NULL,
		    EMPRESA_ESTADO_CD VARCHAR(2) NULL,
		    EMPRESA_ESTADO VARCHAR(200) NULL,
		    EMPRESA_TIPO_CD VARCHAR(2) NULL,
		    EMPRESA_TIPO VARCHAR(200) NULL,
		    ESTADO_HABIDO_CD VARCHAR(2) NULL,
		    ESTADO_HABIDO VARCHAR(200) NULL,
		    CIIU_V3 VARCHAR(4) NULL,
		    CIIU_V4 VARCHAR(4) NULL,
		    SECTOR VARCHAR(200) NULL,
		    DESCR_CIIU VARCHAR(200) NULL, 
		    ACTIV_COMERCIO_EXTERIOR_CD VARCHAR(2) NULL,
		    ACTIV_COMERCIO_EXTERIOR VARCHAR(200) NULL,
		    REG_PROPIEDAD_CD VARCHAR(2) NULL,
		    REG_PROPIEDAD VARCHAR(200) NULL,
		    AGREGADO INT NULL,
		    FLAG INT NULL,
		    USUCRE VARCHAR(35) NULL, 
            FECCRE VARCHAR(35) NULL, 
            USUREG VARCHAR(35) NULL, 
            FECREG VARCHAR(35) NULL, 
            FECENV VARCHAR(35) NULL,
		    PRIMARY KEY(ID),
		    CONSTRAINT T_MARCO_UK UNIQUE(RUC),
		    CONSTRAINT T_MARCO_FK_T_UBIGEO FOREIGN KEY(UBIGEO) REFERENCES T_UBIGEO(UBIGEO)
        )
    </sql_tablas>
    <sql_tablas>
    	CREATE TABLE T_SEGMENTACION (
			ID INT NOT NULL,
			COD_SEDE_OPERATIVA VARCHAR(2) NULL,
			CCDD CHAR(2) NULL,
			CCPP CHAR(2) NULL,
			CCDI CHAR(2) NULL,
			RUC VARCHAR(11) NOT NULL,
			EQUIPO VARCHAR(3) NULL,
			RUTA VARCHAR(3) NULL,
			PERIODO INT NULL,
			ESTADO INT NULL,
            USUCRE VARCHAR(35) NULL, 
            FECCRE VARCHAR(35) NULL, 
            USUREG VARCHAR(35) NULL, 
            FECREG VARCHAR(35) NULL, 
            FECENV VARCHAR(35) NULL,
			PRIMARY KEY(ID),
			CONSTRAINT T_SEGMENTACION_UK UNIQUE(COD_SEDE_OPERATIVA,CCDD,CCPP,CCDI,RUC,EQUIPO,RUTA,PERIODO)
		)
	</sql_tablas>
    <sql_tablas>
		CREATE TABLE T_PERSONAL (
 			DNI VARCHAR(8) PRIMARY KEY,
 			TELEFONO VARCHAR(30), 	
			EMAIL VARCHAR(30),  
			ESTADO CHAR(1), 
			NOMBRES VARCHAR(150), 
			APATERNO VARCHAR(150), 
			AMATERNO VARCHAR(150),
			CORREO VARCHAR(100),
			ID INT NULL,	
			USUCRE VARCHAR(35) NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG VARCHAR(35) NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL		
		)

    </sql_tablas>
    <sql_tablas>
		CREATE TABLE T_PERMISO (
			ID INTEGER PRIMARY KEY,
			DESCRIPCION VARCHAR(200),
            USUCRE VARCHAR(35) NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG VARCHAR(35) NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL
		)

    </sql_tablas>
    <sql_tablas>
		CREATE TABLE T_ROL (
			ID INTEGER PRIMARY KEY,
			DESCRIPCION VARCHAR(200),
			ACCESO INT,
            USUCRE VARCHAR(35) NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG VARCHAR(35) NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL
		)

    </sql_tablas>
    <sql_tablas>
		CREATE TABLE T_ROLES_PERMISOS (
			ID INTEGER PRIMARY KEY,
			ROL_ID INTEGER NOT NULL,
			PERMISO_ID INTEGER NOT NULL,
            USUCRE VARCHAR(35) NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG VARCHAR(35) NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL,
            CONSTRAINT T_ROLES_PERMISOS_FK_T_ROL FOREIGN KEY(ROL_ID) REFERENCES T_ROL(ID),
            CONSTRAINT T_ROLES_PERMISOS_FK_T_PERMISO FOREIGN KEY(PERMISO_ID) REFERENCES T_PERMISO(ID)
		)

    </sql_tablas>
    <sql_tablas>
        CREATE TABLE T_USUARIO (
            ID INT NOT NULL,
            USUARIO VARCHAR(50) NULL,
            CLAVE VARCHAR(50) NULL,
            CARGO_ID INT NOT NULL,
            COD_SEDE_OPERATIVA VARCHAR(3) NULL,
            CCDD CHAR(2) NULL,
			CCPP CHAR(2) NULL,
			CCDI CHAR(2) NULL,
            EQUIPO VARCHAR(3) NULL,
			RUTA VARCHAR(3) NULL,
			ESTADO INT DEFAULT 1,
			SUPERVISA VARCHAR(3) NULL,
            USUCRE VARCHAR(35) NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG VARCHAR(35) NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL,
            PRIMARY KEY (ID),
            CONSTRAINT T_USUARIO_FK_T_ROL FOREIGN KEY(CARGO_ID) REFERENCES T_ROL(ID)
        )

    </sql_tablas>
    <sql_tablas>
		CREATE TABLE T_USUARIOS_PERSONAL (
			ID INT NULL,
			DNI VARCHAR(8) NOT NULL,
			USUARIO_ID INTEGER NOT NULL,
            USUCRE INT NULL,
            FECCRE VARCHAR(35) NULL,
            USUREG INT NULL,
            FECREG VARCHAR(35) NULL,
            FECENV VARCHAR(35) NULL,
			PRIMARY KEY(DNI, USUARIO_ID),
            CONSTRAINT T_USUARIOS_PERSONAL_PK UNIQUE(DNI, USUARIO_ID),
            CONSTRAINT T_USUARIOS_PERSONAL_FK_T_PERSONAL FOREIGN KEY(DNI) REFERENCES T_PERSONAL(DNI),
            CONSTRAINT T_USUARIOS_PERSONAL_FK_T_USUARIO FOREIGN KEY(USUARIO_ID) REFERENCES T_USUARIO(ID)
		)

    </sql_tablas>
    <sql_tablas>
		CREATE TABLE T_SEDES_OPERATIVAS(
			COD_SEDE VARCHAR(3) NOT NULL,
			SEDE VARCHAR(200) NOT NULL,
			ID INT NULL,
			USUCRE INT NULL, 
			FECCRE VARCHAR(35) NULL, 
			USUREG INT NULL, 
			FECREG VARCHAR(35) NULL, 
			FECENV VARCHAR(35) NULL,
			PRIMARY KEY(COD_SEDE)
		)		

    </sql_tablas>
    <sql_tablas>
        CREATE TABLE T_SEDES_OPERATIVAS_UBIGEO(
			COD_SEDE VARCHAR(3) NOT NULL,
			UBIGEO CHAR(6) NOT NULL,
			ID INT NULL,
			USUCRE INT NULL, 
			FECCRE VARCHAR(35) NULL, 
			USUREG INT NULL, 
			FECREG VARCHAR(35) NULL, 
			FECENV VARCHAR(35) NULL,
			PRIMARY KEY(COD_SEDE, UBIGEO),
			CONSTRAINT T_SEDES_OPERATIVAS_UBIGEOS_FK_T_SEDES_OPERATIVAS FOREIGN KEY(COD_SEDE) REFERENCES T_SEDES_OPERATIVAS(COD_SEDE),
			CONSTRAINT T_SEDES_OPERATIVAS_UBIGEOS_FK_T_UBIGEO FOREIGN KEY(UBIGEO) REFERENCES T_UBIGEO(UBIGEO)
		)

    </sql_tablas>
</sqldatabase> 
