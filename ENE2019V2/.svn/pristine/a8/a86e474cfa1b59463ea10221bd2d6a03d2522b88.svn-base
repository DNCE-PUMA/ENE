		LinearLayout botones = createButtonSection(btnAceptar, btnCancelar);

		ScrollView contenedor = createForm();
		LinearLayout form = (LinearLayout) contenedor.getChildAt(0);

		form.addView(q0);
		form.addView(q2);
		form.addView(q1);
		form.addView(q3);
		form.addView(q4);
		form.addView(q5);
		form.addView(botones);

		return contenedor;
	}

