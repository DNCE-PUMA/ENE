		}
		Field[] campos = bean.getPracticeFields();
		for (int i = 0; i < campos.length; i++) {
			// if (campos[i].get(bean) != null) {
			puntaje = puntaje.add(BigDecimal.ONE);
			// Log.e(getClass().getSimpleName(), bean.getClass().getSimpleName()
			// + " - " + campos[i].getName());
			// }
		}
		return puntaje;
	}
