	private boolean obtenerEstado(Detalle detalle) {
		// Azul al completar el No.

		if (!Util.esDiferente(detalle.c6p616a, 1)) {
			if (detalle.c6p616b != null) {
				return true;
			}
		}

		if (!Util.esDiferente(detalle.c6p616a, 2)) {
				return true;
		}
		return false;
	}
	
